<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/linearLayout6"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <ImageView
        android:id="@+id/hsplogo"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_marginEnd="10dp"
        android:layout_marginRight="10dp"
        app:layout_constraintBottom_toBottomOf="@+id/hspstart"
        app:layout_constraintEnd_toStartOf="@+id/hspname"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@mipmap/ic_launcher" />

    <TextView
        android:id="@+id/hspname"
        android:layout_width="134dp"
        android:layout_height="128dp"
        android:gravity="left|center_vertical"
        android:text="医院名称"
        android:textSize="24sp"
        app:layout_constraintEnd_toStartOf="@+id/hspstart"
        app:layout_constraintStart_toEndOf="@+id/hsplogo"
        app:layout_constraintTop_toTopOf="@+id/hspstart" />

    <TextView
        android:id="@+id/hspstart"
        android:layout_width="0dp"
        android:layout_height="136dp"
        android:gravity="center"
        android:text="医院星级"
        android:textSize="24sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/hspname"
        app:layout_constraintTop_toTopOf="parent" />

</androidx.constraintlayout.widget.ConstraintLayout>