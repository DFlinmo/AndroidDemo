<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/linearLayout12"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical">

    <TextView
        android:id="@+id/textView1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="20dp"
        android:text="车\t\t牌\t\t号："
        android:textSize="18sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textView2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="4dp"
        android:layout_marginTop="16dp"
        android:text="车\t\t位\t\t号 :"
        android:textSize="18sp"
        app:layout_constraintStart_toStartOf="@+id/textView1"
        app:layout_constraintTop_toBottomOf="@+id/textView1" />

    <TextView
        android:id="@+id/textView3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="24dp"
        android:text="停\t车\t卡\t号\t："
        android:textSize="18sp"
        app:layout_constraintStart_toStartOf="@+id/textView2"
        app:layout_constraintTop_toBottomOf="@+id/textView2" />

    <TextView
        android:id="@+id/textView4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="16dp"
        android:text="车\t主\t姓\t名\t："
        android:textSize="18sp"
        app:layout_constraintStart_toStartOf="@+id/textView3"
        app:layout_constraintTop_toBottomOf="@+id/textView3" />

    <TextView
        android:id="@+id/textView5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="20dp"
        android:text="车主手机号\t："
        android:textSize="18sp"
        app:layout_constraintStart_toStartOf="@+id/textView4"
        app:layout_constraintTop_toBottomOf="@+id/textView4" />

    <TextView
        android:id="@+id/textView6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="24dp"
        android:text="住\t户\t地\t址："
        android:textSize="18sp"
        app:layout_constraintStart_toStartOf="@+id/textView7"
        app:layout_constraintTop_toBottomOf="@+id/textView7" />

    <TextView
        android:id="@+id/textView7"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="28dp"
        android:text="住\t户\t姓\t名："
        android:textSize="18sp"
        app:layout_constraintStart_toStartOf="@+id/textView5"
        app:layout_constraintTop_toBottomOf="@+id/textView5" />

    <EditText
        android:id="@+id/carnum"
        android:layout_width="236dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="12dp"
        app:layout_constraintStart_toEndOf="@+id/textView1"
        app:layout_constraintTop_toTopOf="parent" />

    <EditText
        android:id="@+id/parkingSpace"
        android:layout_width="236dp"
        android:layout_height="wrap_content"
        app:layout_constraintStart_toStartOf="@+id/carnum"
        app:layout_constraintTop_toBottomOf="@+id/carnum" />

    <EditText
        android:id="@+id/parkingCardNumber"
        android:layout_width="236dp"
        android:layout_height="wrap_content"
        app:layout_constraintStart_toStartOf="@+id/parkingSpace"
        app:layout_constraintTop_toBottomOf="@+id/parkingSpace" />

    <EditText
        android:id="@+id/ownerSName"
        android:layout_width="236dp"
        android:layout_height="wrap_content"
        app:layout_constraintStart_toStartOf="@+id/parkingCardNumber"
        app:layout_constraintTop_toBottomOf="@+id/parkingCardNumber" />

    <EditText
        android:id="@+id/ownersTel"
        android:layout_width="236dp"
        android:layout_height="wrap_content"
        app:layout_constraintStart_toStartOf="@+id/ownerSName"
        app:layout_constraintTop_toBottomOf="@+id/ownerSName" />

    <EditText
        android:id="@+id/residentName"
        android:layout_width="236dp"
        android:layout_height="wrap_content"
        app:layout_constraintStart_toStartOf="@+id/ownersTel"
        app:layout_constraintTop_toBottomOf="@+id/ownersTel" />

    <EditText
        android:id="@+id/address"
        android:layout_width="236dp"
        android:layout_height="wrap_content"
        app:layout_constraintStart_toStartOf="@+id/residentName"
        app:layout_constraintTop_toBottomOf="@+id/residentName" />


</androidx.constraintlayout.widget.ConstraintLayout>